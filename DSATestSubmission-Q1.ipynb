   "source": [
    "s=int(input())\n",
    "\n",
    "def binary(s):\n",
    "    i=1\n",
    "    j=s//2\n",
    "    ans=0\n",
    "    while i <= j:\n",
    "        mid = (i+j) // 2\n",
    "        t=(mid)*mid\n",
    "        if t<=s:\n",
    "            ans=mid\n",
    "            i = mid + 1\n",
    "        else:\n",
    "            j = mid - 1\n",
    "    print(ans)\n",
    "    \n",
    "binary(s)\n"
   ]
